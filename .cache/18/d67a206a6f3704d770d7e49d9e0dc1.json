ng) {\n            return <h1>Loading ....</h1>\n        }\n        const {name, animal, breed, location,description, media} = this.state;\n        return (\n            <div className=\"details\">\n            <Carousel media={media} />\n                <div>\n                    <h1>{name}</h1>\n                    <h2>{animal} - {breed} - {location}</h2>\n                    <p>{description}</p>\n                </div>\n\n            </div>\n        );\n    }\n}\n\nexport default Details;\n"},"lineCount":null}},"error":null,"hash":"e26441633ecca17802895859e97e95f7","cacheData":{"env":{"API_KEY":"1AwMOMigZuZpxBPY9ImFYlr0TgNqhtXK8uIRUv8bnMzc1zHaDN","API_SECRET":"CishoguQPUy1NGBvPb8YJ6tjmXBX0K65HvSjdnmx"}}}